        <SQLTask:SqlTaskData
          SQLTask:Connection="{5ED08E0F-ABBB-4080-8967-056CD4D307DB}"
          SQLTask:SqlStatementSource="UPDATE  eca_local.dbo.location &#xA;SET     [locationiso-2] = &#xA;        (&#xA;          SELECT [Country or Area Code GENC 2] &#xA;          FROM sample_data.dbo.mrdCountryAreasList b &#xA;          WHERE eca_local.dbo.location.locationiso = b.[Country or Area Code GENC 3]&#xA;        )&#xA;WHERE eca_local.dbo.location.locationtypeid = 3 AND eca_local.dbo.location.locationiso is not null" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
