        <SQLTask:SqlTaskData
          SQLTask:Connection="{FABB5F0D-8E1D-4AF6-ADC7-4D8DB4D0BE3A}"
          SQLTask:SqlStatementSource="UPDATE a&#xA;SET   a.city_locationid = b.locationid &#xA;FROM    location a&#xA;        INNER JOIN location b&#xA;            ON UPPER(a.city) = (b.locationName) and b.locationtypeid = 5 AND &#xA;&#x9;&#x9;&#x9;((b.country_locationid IS NULL AND a.country_locationid is null) &#xA;&#x9;&#x9;&#x9; OR (b.country_locationid = a.country_locationid)) &#xA;&#x9;&#x9;&#x9;AND ((b.region_locationid IS NULL and a.region_locationid is null) &#xA;&#x9;&#x9;&#x9; OR (b.region_locationid = a.region_locationid))&#xA;WHERE a.locationtypeid &lt;&gt; 5 AND a.city IS NOT NULL" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
