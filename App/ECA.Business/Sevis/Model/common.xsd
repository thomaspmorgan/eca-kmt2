	<xs:complexType name="TravelNullableType">
		<xs:annotation>
			<xs:documentation>Travel information type Nullable</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PassportNumber" minOccurs="0">
