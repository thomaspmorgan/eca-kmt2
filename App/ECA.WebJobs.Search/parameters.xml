<?xml version="1.0" encoding="utf-8" ?>
<parameters >

  <parameter name="search.ApiKey" description="Azure search api key.">
    <parameterEntry kind="XmlFile"
                    scope="\\web.config$"
                    match="/configuration/appSettings/add[@key='search.ApiKey']/@value" />
  </parameter>

  <parameter name="search.ServiceName" description="Azure search service name.">
    <parameterEntry kind="XmlFile"
                    scope="\\web.config$"
                    match="/configuration/appSettings/add[@key='search.ServiceName']/@value" />
  </parameter>

</parameters>

